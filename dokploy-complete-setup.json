{
  "application": {
    "name": "certigraph-backend",
    "description": "AI-powered certification exam study platform backend",
    "git": {
      "repository": "git@github.com:myaji35/15_CertiGraph.git",
      "branch": "main",
      "buildPath": "/backend",
      "dockerfile": "Dockerfile"
